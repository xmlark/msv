<case name="test for day">
	<value> ---01 </value>
	<value> ---02 </value>
	<value> ---05 </value>
	<value> ---28 </value>
	<value> ---29 </value>
	<value> ---30 </value>
	<value> ---31 </value>
	
	<value> ---10  </value>
	<value> ---10-14:00 </value>
	<value> ---10-08:00 </value>
	<value> ---10Z </value>
	<value> ---10+05:00 </value>
	<value> ---10+09:00 </value>
	
	<answer for="gDay"			>ooooooo oooooo</answer>
	
	<facets><combination>
		<combination mode="or">
          <!-- note:
            1. the below string encodes a boolean for every "value" above, o if the pattern matches, . if not
            2. very nonobvious and no idea why:
               in testDataType() there is a "pattern" iteration that runs all test cases
               with every facet being either enabled and disabled i.e. 4 times in total here
               => "wrong" test input must not be excluded *only* by facet - because there
                  is always a mode where every facet is disabled - but by the type's format
          -->

			<facet answer=		"ooo.... ......" name="pattern" value="---0[1-9]" />
			<facet answer=		"....... oooooo" name="pattern" value=".*10.*" />
		</combination>
<!--	order relation is broken
		note that value of "value" attribute is modified to make it commentable.
		remove \ when you use them.
		<combination mode="or">
			<facet answer=		"....... ......" name="enumeration" value="-\-\-01Z" />
			<facet answer=		"....... .o...." name="enumeration" value="-\-\-09+10:00" />
		</combination>
		<choice>
			<facet answer=		"..ooooo oooooo" name="minInclusive" value="-\-\-05" />
			<facet answer=		"...oooo oooooo" name="minExclusive" value="-\-\-05" />
			<facet answer=		"...oooo o....." name="minInclusive" value="-\-\-10" />
		</choice>
-->
	</combination></facets>
	
	<wrongs>
		<value> ---10-5:00 </value>
		<value> 10 </value>
		<value> ---1 </value>
        <!-- <value> - - -00 </value> -->
        <!-- <value> - - -32 </value> -->
	</wrongs>
</case>

